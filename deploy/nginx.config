server {
    listen 80; 
    listen [::]:80;
    server_name tnki.octopuese.xyz;
    return 301 https://$server_name$request_uri;
}

server {
    listen 443 ssl;

    root /var/www/Tnki/client/dist;
    index index.html index.htm;

    server_name tnki.octopuese.xyz;
    ssl_certificate /etc/letsencrypt/live/tnki.octopuese.xyz/cert.pem;
    ssl_certificate_key /etc/letsencrypt/live/tnki.octopuese.xyz/privkey.pem;

    location / {
	      try_files $uri $uri/ =404;
    }
}
